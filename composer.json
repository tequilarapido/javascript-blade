{
    "name": "tequilarapido/javascript-blade",
    "description": "",
    "homepage": "https://github.com/tequilarapido/javascript-blade",
    "license": "MIT",
    "authors": [
        {
            "name": "Nassif Bourguig",
            "email": "nbourguig@gmail.com",
            "homepage": "https://github.com/nbourguig",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": ">=5.4 <=6.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*",
        "orchestra/testbench": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Tequilarapido\\JavascriptBlade\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tequilarapido\\JavascriptBlade\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tequilarapido\\JavascriptBlade\\JavascriptBladeProvider"
            ]
        }
    }
}
